{
  "expo": {
    "name": "CleanyTube",
    "scheme": "cleanytube",
    "slug": "cleanytube",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleidentifier": "com.cleanytube.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cleanytube.app",
      "versionCode": "2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "26e3d5f8-91ab-4571-8528-82fd92f528a8"
      }
    },
    "owner": "sasmel",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/26e3d5f8-91ab-4571-8528-82fd92f528a8"
    }
  },
  "packagerOpts": {
    "config": "rn-cli.config.js"
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "google-play.service-account.json",
        "track": "internal"
      },
      "ios": {}
    }
  }
}
